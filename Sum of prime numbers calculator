def prime(n):
    for i in range(2, n // 2 + 1):
        if (n % i == 0):
            return (0)
    return (1)

print("To calculate the sum of prime numbers between a given range")
a = int(input("Enter the starting point:"))
b = int(input("Enter the end point:"))
result = []
for i in range(a, b + 1):
    if (prime(i)):
        result.append(i)
print(sum(result))
